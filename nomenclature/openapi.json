				"responses": {
					"200": {
						"description": "Successful",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Revisions"
								}
							},
							"application/xml": {
								"schema": {
									"$ref": "#/components/schemas/Revisions"
								}
							}
						}
					}
				}
			}
		},
